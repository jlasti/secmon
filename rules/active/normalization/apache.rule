type=single
ptype=regexp
pattern=^([A-Za-z0-9: ]*) (\S+) (?P<tag>.*?) (?P<remote_ip>.*?) (\S+) (\S+) \[([\w:\/]+\s[+\-]\d{4})\] \"(?P<request_method>.*?) (?P<request_url>.*?) (?<protocol>HTTP.?)\/(?P<request_version>.*)?\" (?P<status_code>.*?) (\d+|-)\s?"?([^"]*)"?\s?\"(?P<referrer>.*?)\" \"(?P<user_agent>.*?)\"
desc=httpd accesslog from host IP $+{remote_ip}
action=write /var/log/secmon/__secInput $1 $2 CEF:0|Apache|httpd||$+{status_code}|HTTP Request|Unknown| app=$+{protocol} src=$+{remote_ip} request_method=$+{request_method} request=$+{request_url} requestClientApplication=$+{user_agent} rawEvent=$0
